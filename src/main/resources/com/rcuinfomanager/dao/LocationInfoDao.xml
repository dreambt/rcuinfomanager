<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rcuinfomanager.dao.LocationInfoDao">

    <insert id="saveLocationInfo" parameterType="com.rcuinfomanager.model.LocationInfo">
        insert into locationinfo (userName,longitude,latitude,recordTime)
          values(#{userName},#{longitude}, #{latitude}, #{recordTime})
    </insert>
</mapper>